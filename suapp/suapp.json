{
    "datasource": {
        "location": "~/.susm/data/",
        "type": "Plank"
    },
    "log": {
        "filemode": "w",
        "filename": "~/.suapp/log/suapp.log",
        "format": "%(asctime)s %(levelname)s %(name)s %(message)s",
        "level": "INFO",
        "modules": {
            "base": {
                "level": "INFO"
            },
            "httpd": {
                "filename": "~/.suapp/log/httpd.accces_log",
                "level": "DEBUG"
            }
        }
    },
    "modules": {
        "base": {}
    },
    "name": "SU Demo Application",
    "shortname": "suapp"
}