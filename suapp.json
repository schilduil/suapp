{
    "name": "SU Demo Application",
    "shortname": "suapp",
    "log": {
        "filename": "~/.suapp/log/suapp.log",
        "filemode": "w",
        "format": "%(asctime)s %(levelname)s %(name)s %(message)s",
        "level": "INFO",
        "modules": {
            "base": {
                "level": "INFO"
            }
        }
    },
    "datasource": {
        "type": "Plank",
        "location": "~/.susm/data/"
    },
    "modules": {
        "base": {}
    }
}
